{
    'presets': ['es2015-no-commonjs'],
    'plugins': [
        'transform-inline-environment-variables',
        'transform-export-extensions',
        'transform-object-assign',
        'transform-object-rest-spread'
    ],
    'sourceMaps': true,

    'env': {
        'bundle': {
            'plugins': [
                ['transform-runtime', {
                    'polyfill': false,
                    'regenerator': false
                } ]
            ]
        },
        'cjs': {
            'plugins': ['transform-es2015-modules-commonjs']
        }
    }
}
